{"code":"import React, { useState, createElement } from \"react\";\r\nimport { useId } from \"@reach/auto-id\";\r\nimport { styled, setup } from \"goober\";\r\nimport Editor from \"./Editor\";\r\nimport Result from \"./Result\";\r\nimport { ThemeProvider, useTheme } from \"./utils/ThemeProvider\";\r\nimport media from \"./utils/media\";\r\nimport Draggable from \"./Draggable\";\r\nsetup(createElement, undefined, useTheme);\r\nconst StyledDraggable = styled(Draggable) `\n  border: 0.1em solid ${(props) => props.theme.container.borderColor};\n  display: flex;\n  min-height: ${(props) => props.theme.container.minHeight};\n\n  ${media.phone} {\n    flex-direction: column;\n  }\n`;\r\nconst Playground = ({ id: userId, initialSnippet, defaultEditorTab = \"markup\", defaultResultTab = \"result\", transformJs = false, presets = [], theme, mode = \"light\", }) => {\r\n    const [snippet, setSnippet] = useState(initialSnippet);\r\n    const id = useId(userId);\r\n    const onSnippetChange = (changed, type) => {\r\n        setSnippet((snippet) => ({\r\n            ...snippet,\r\n            [type]: changed,\r\n        }));\r\n    };\r\n    return (React.createElement(ThemeProvider, { userTheme: theme, mode: mode },\r\n        React.createElement(\"div\", { className: \"playground\" },\r\n            React.createElement(StyledDraggable, { leftChild: (width) => (React.createElement(Editor, { width: width, code: snippet, defaultTab: defaultEditorTab, onChange: onSnippetChange })), rightChild: (width) => (React.createElement(Result, { width: width, id: id, snippet: snippet, defaultTab: defaultResultTab, transformJs: transformJs, presets: presets })) }))));\r\n};\r\nexport default Playground;\r\n//# sourceMappingURL=Playground.js.map","references":["/Users/bidishamondal/Desktop/Projects/frontend-code-playground/node_modules/@types/react/index.d.ts","/Users/bidishamondal/Desktop/Projects/frontend-code-playground/node_modules/@reach/auto-id/dist/reach-auto-id.cjs.d.ts","/Users/bidish